.well.col-lg-12
  .resumeForm
    %h1 Create Your Resume
    %h2 Choose Elements to Add to Resume
    %br
    %p.sectionEnd
    =form_tag resumes_create_path, :method => :post do
      =text_field_tag :title, "", placeholder: "Resume Title", class: "form-control"
      %br
      %h3 Skills
      %br
      - for skill in @master.skills
        %p.skill
          = check_box_tag "skill_ids[]", skill.id
          = skill.name
      %br
      = button_tag "Add to Resume", class: "btn btn-default"
